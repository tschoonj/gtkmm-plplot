prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: Gtkmm-PLplot
Description: a scientific plotting widget collection for Gtkmm
Version: @VERSION@
Requires: gtkmm-4.0 > 4.0.0
Requires.private: @PLPLOT_CXX@
Conflicts:
Libs: -L${libdir} -lgtkmm-plplot-@GTKMM_PLPLOT_API_VERSION@
Cflags: -I${includedir}/@GTKMM_PLPLOT_MODULE_NAME@ -I${libdir}/@GTKMM_PLPLOT_MODULE_NAME@/include @BOOST_CPPFLAGS@
